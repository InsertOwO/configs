        "transition-duration": 250,
        "modules": [
                {
                        "type": "screenshare",
                        "tooltip": true,
                        "tooltip-icon-size": 24
                },
                {
                        "type": "audio-in",
                        "tooltip": true,
                        "tooltip-icon-size": 24
                }
        ]
},
